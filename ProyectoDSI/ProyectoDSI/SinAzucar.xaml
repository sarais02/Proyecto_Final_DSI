<Page
    x:Class="ProyectoDSI.SinAzucar"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ProyectoDSI"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Page.Resources>
        <Style TargetType="Image">
            <Setter Property="AllowDrop" Value="True"/>
            <Setter Property="Stretch" Value="Fill"/>
        </Style>
        <Style TargetType="StackPanel" x:Name="panelesInfo">
            <Setter Property="BorderBrush" Value="White"/>
            <Setter Property="BorderThickness" Value="2"/>
            <Setter Property="Background" Value="#c238eb"/>
            <Setter Property="HorizontalAlignment" Value="Left"/>
            <Setter Property="VerticalAlignment" Value="Center"/>
            <Setter Property="Width" Value="100"/>
        </Style>
        <Style TargetType="TextBlock" x:Name="textInfoPanel">
            <Setter Property="VerticalAlignment" Value="Center"/>
            <Setter Property="HorizontalAlignment" Value="Center"/>
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="FontFamily" Value="MV Boli"/>
        </Style>
    </Page.Resources>



    <Grid Background="RoyalBlue" KeyDown="Grid_KeyDown">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="282*"/>
            <ColumnDefinition Width="1047*"/>
            <ColumnDefinition Width="170*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="21*"/>
            <RowDefinition Height="179*"/>
        </Grid.RowDefinitions>

        <Viewbox  Grid.Row="1" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
            <Grid VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Margin="50,0,0,500">
                <TextBlock HorizontalAlignment="Right" TextWrapping="Wrap"
                   FontSize="30" TextAlignment="Center" VerticalAlignment="Top"
                   Foreground="Cyan" FontFamily="MV Boli" FontWeight="Bold" 
                   Grid.Row="1" Width="140" x:Uid="InfoText"/>

            </Grid>
        </Viewbox>
        
        <Viewbox HorizontalAlignment="Left">
            <Button x:Name="PauseButton" Click="PauseButton_Click" x:Uid="PauseButtonText"
                Background="LightPink" Foreground="Black"
                Margin="20,20,0,0" FontSize="25"
                FontFamily="MV Boli" CornerRadius="10" BorderThickness="4"
                BorderBrush="White" Width="120" Height="58"/>
        </Viewbox>

        <Viewbox  Grid.Row="1"  Grid.Column="2" VerticalAlignment="Top" Margin="10,10,10,0">
            <Border BorderThickness="10" BorderBrush="#FF040E52" Background="Transparent"
                CornerRadius="10" Height="80" >
                <TextBlock x:Name="CountDown" Text="00:00" FontSize="45" Foreground="White" />
            </Border>
        </Viewbox>
        <!--Paneles de informacion cartas-->
        <Viewbox Grid.Row="1" Grid.ColumnSpan="3" Margin="0,200,0,0" >
            <Grid >
                <StackPanel Orientation="Horizontal" Spacing="650">
                    <StackPanel  x:Name="PanelInfoJugador"  Style="{StaticResource panelesInfo}" Margin="125,0,0,0" Visibility="Collapsed"  >
                        <TextBlock  x:Name="CartaInfoJugador" Text="Regaliz" Style="{StaticResource textInfoPanel}" FontSize="24"/>
                        <TextBlock x:Name="RangoCartaJugador" Text="Rango: 7" Style="{StaticResource textInfoPanel}" FontSize="16"/>
                        <Image x:Name="ImagenCartaJugador" Source="Assets/regaliz.png" Width="50"></Image>
                        <TextBlock x:Name="InfoCartaJugador"  Style="{StaticResource textInfoPanel}"
                           TextWrapping="Wrap" Text="Tiene su punto débil expuesto, la yema, 
                           pero para poder acabar con él deberás cogerlo primero y gracias a su disposición de la clara puede arrastrarse muy rápido"  FontSize="8"/>
                    </StackPanel>
                    <StackPanel  x:Name="PanelInfoEnemigo" Style="{StaticResource panelesInfo}" Margin="0,0,45,0" Visibility="Collapsed" >
                        <TextBlock  x:Name="CartaInfoEnemigo" Text="Regaliz"   FontSize="24" Style="{StaticResource textInfoPanel}"/>
                        <TextBlock x:Name="RangoCartaEnemigo" Text="Rango: 7" Style="{StaticResource textInfoPanel}" FontSize="16"/>
                        <Image x:Name="ImagenCartaEnemigo" Source="Assets/regaliz.png" Width="50"></Image>
                        <TextBlock x:Name="InfoCartaEnemigo" Style="{StaticResource textInfoPanel}" 
                           TextWrapping="Wrap" Text="Tiene su punto débil expuesto, la yema, 
                           pero para poder acabar con él deberás cogerlo primero y gracias a su disposición de la clara puede arrastrarse muy rápido"  FontSize="8"/>
                    </StackPanel>
                </StackPanel>
            </Grid>
        </Viewbox>
        
